<% provide :title, t("admin.allcategory") %>
<h1><%= t "admin.allcategory" %></h1>
<ul class="categories">
  <% @categories.each do |category| %>
    <li>
      <%= category.name %>
      <%= link_to t("admin.edit"), edit_admin_category_path(category) %>
    </li>
  <% end %>
</ul>
<%= will_paginate @categories %>
